{
  "expo": {
    "name": "atenas-agenda",
    "slug": "atenas-agenda",
    "scheme": "atenas-agenda",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
